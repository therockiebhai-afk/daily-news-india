{
  "builds": [
    { "src": "api/fetch.py", "use": "@vercel/python@3.1.57" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/fetch", "dest": "api/fetch.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
